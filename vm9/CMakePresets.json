{
  "version": 8,
  "configurePresets": [
    {
      "name": "svlm_project",
      "displayName": "GCC 11.2.0 x86_64-linux-gnu SVLM PROJECT",
      "description": "Using compilers: C = /usr/bin/gcc, CXX = /usr/bin/g++",
      "binaryDir": "/dev/shm/out/build/${presetName}",
      "cacheVariables": {
        "CMAKE_INSTALL_PREFIX": "/dev/shm/out/install/${presetName}",
        "CMAKE_C_COMPILER": "/usr/bin/gcc",
        "CMAKE_CXX_COMPILER": "/usr/bin/g++",
        "CMAKE_BUILD_TYPE": "Debug"
      }
    }
  ]
}